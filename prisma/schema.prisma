generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Acceso {
  id            String   @id @default(uuid())
  nombre_apellido           String
  url           String
  fecha_hora_ingreso DateTime @default(now())
  fecha_hora_egreso  DateTime?
  activo        Boolean  @default(true)
}

